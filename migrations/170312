ALTER TABLE `channel` CHANGE `window_id` `id_window` INT(11) NOT NULL;

ALTER TABLE `channel` CHANGE `internalId` `id_widget` INT(11) NOT NULL;

UPDATE `channel`
LEFT JOIN `widget_to_channel` ON `channel`.`id`= `widget_to_channel`.`channel_id`
SET `channel`.`id_widget`  = `widget_to_channel`.`widget_id`;

ALTER TABLE `channel` RENAME `showcase`;

DROP TABLE `widget_to_channel`;
